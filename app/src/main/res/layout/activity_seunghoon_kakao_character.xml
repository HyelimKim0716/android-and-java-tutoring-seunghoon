                <TextView
                    android:id="@+id/sh_kakao_tvMuziSizeValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SizeValue" />

            </LinearLayout>


        </LinearLayout>


    </LinearLayout>

    <LinearLayout
