<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".seunghoon.SeunghoonMainActivity">

    <Button
        android:id="@+id/seunghoonMain_btnLinearLayout"
        style="@style/Button.CustomNormal"
        android:text="Linear Layout"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <Button
        android:id="@+id/seunghoonMain_btnDataType"
        style="@style/Button.CustomNormal"
        android:text="Data Type"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnLinearLayout" />

    <Button
        android:id="@+id/seunghoonMain_btnDataTypeAssignment"
        style="@style/Button.CustomNormal"
        android:text="Data Type Assignment"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnDataType"/>

    <Button
        android:id="@+id/seunghoonMain_btnControl"
        style="@style/Button.CustomNormal"
        android:text="Control"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnLinearLayout"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/seunghoonMain_btnRepeat"
        style="@style/Button.CustomNormal"
        android:text="Repeat"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnDataType"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnControl" />

    <Button
        android:id="@+id/seunghoonMain_btnMethod1"
        style="@style/Button.CustomNormal"
        android:text="Method1"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnDataTypeAssignment"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnRepeat" />

    <Button
        android:id="@+id/seunghoonMain_btnMethod2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="Method2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnControl" />

    <Button
        android:id="@+id/seunghoonMain_btnMethod3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="Method3"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnMethod2"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnRepeat" />

    <Button
        android:id="@+id/seunghoonMain_btnSchedule"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="Schedule"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnMethod3"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnMethod1" />

    <Button
        android:id="@+id/seunghoonMain_btnCarClass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="CarClass"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnMethod2" />

    <Button
        android:id="@+id/seunghoonMain_btnDogClass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="Dog Class"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnCarClass"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnMethod3" />

    <Button
        android:id="@+id/seunghoonMain_btnInheritance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="Inheritance"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnDogClass"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnSchedule" />

    <Button
        android:id="@+id/seunghoonMain_btnInterface"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="Interface"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnCarClass" />

    <Button
        android:id="@+id/seunghoonMain_btnInterface2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="Interface"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnInterface"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnDogClass" />

    <Button
        android:id="@+id/seunghoonMain_btnDog2Class"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="Dog2 Class"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnInterface2"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnInheritance" />

    <Button
        android:id="@+id/seunghoonMain_btnKakao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="Kakao"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnInterface" />

    <Button
        android:id="@+id/seunghoonMain_btnPlayer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="Player"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnKakao"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnInterface2" />

    <Button
        android:id="@+id/seunghoonMain_btnRecyclerView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Button.CustomNormal"
        android:text="Recycler View"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnPlayer"
        app:layout_constraintTop_toBottomOf="@id/seunghoonMain_btnDog2Class" />
</android.support.constraint.ConstraintLayout>