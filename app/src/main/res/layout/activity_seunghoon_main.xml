<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".seunghoon.SeunghoonMainActivity">

    <Button
        android:id="@+id/seunghoonMain_btnLinearLayout"
        style="@style/Button.CustomNormal"
        android:text="Linear Layout"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <Button
        android:id="@+id/seunghoonMain_btnDataType"
        style="@style/Button.CustomNormal"
        android:text="Data Type"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/seunghoonMain_btnLinearLayout" />



</android.support.constraint.ConstraintLayout>