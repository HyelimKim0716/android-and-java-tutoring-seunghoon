    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Poomba"/>

    <TextView
        android:id="@+id/hl_animal_tvPoombaValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Poomba Value" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Timon"/>

    <TextView
        android:id="@+id/hl_animal_tvTimonValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Timon Value"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Zazu"/>

    <TextView
        android:id="@+id/hl_animal_tvZazuValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Zazu Value"
        />
